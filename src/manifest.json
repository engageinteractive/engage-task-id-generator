{
  "manifest_version": 2,
  "name": "Engage",
  "description": "A multipurpose extension for use by the Engage team",
  "default_locale": "en",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Engage",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
